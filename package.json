{
  "name": "ssg_pug_postcss_ts_boilerplate",
  "version": "1.0.0",
  "description": "SSG_pug-postcss-ts_boilerplate",
  "config": {
    "feature_name": "feature",
    "path_dist": "./dist"
  },
  "scripts": {
    "bs": "node server.js",
    "lint_style": "stylelint ./src/css/**/*.css",
    "format_js": "eslint --fix --ext .ts ./src/typescript",
    "img_min": "node imgMin.js",
    "webp": "./webp.sh",
    "copy_img_temporary": "cpx \"./src/img/**/*\" ./src/temporary/img/",
    "copy_img": "cpx \"./src/temporary/img/**/*\" $npm_package_config_path_dist/SSG_pug-postcss-ts_boilerplate/$npm_package_config_feature_name/img/",
    "clean": "rimraf $npm_package_config_path_dist/SSG_pug-postcss-ts_boilerplate/$npm_package_config_feature_name",
    "watch_html": "chokidar './src/**/*.pug' -c 'npm run build_html'",
    "watch_json": "chokidar './src/config/*.json' -c 'npm run build_html'",
    "watch_css": "chokidar './src/**/*.css' -c 'npm run build_css'",
    "watch_img": "chokidar './src/img/**/*' -c 'npm run build_img'",
    "build_html": "node create.js -- $npm_package_config_path_dist",
    "build_css": "postcss ./src/css/app.css -o $npm_package_config_path_dist/SSG_pug-postcss-ts_boilerplate/$npm_package_config_feature_name/css/app.css --no-map && npm run lint_style",
    "build_img": "npm run copy_img_temporary && npm run img_min && npm run webp && npm run copy_img",
    "dev": "npm run bs & npm run watch_html & npm run watch_json & npm run watch_css & npm run watch_img & webpack --mode development --watch --env.path_dist=$npm_package_config_path_dist --env.feature_name=$npm_package_config_feature_name --env.ver_js=$npm_package_config_ver_js",
    "build": "npm run clean && npm run build_css && npm run build_html && npm run format_js && webpack --mode production --env.path_dist=$npm_package_config_path_dist --env.feature_name=$npm_package_config_feature_name --env.ver_js=$npm_package_config_ver_js && npm run build_img"
  },
  "license": "ISC",
  "browserslist": [
    "ie 11",
    "last 1 Edge versions",
    "last 1 Firefox versions",
    "last 1 Chrome versions",
    "last 1 Safari versions",
    "iOS >= 9",
    "Android >= 5",
    "last 1 ChromeAndroid versions"
  ],
  "dependencies": {
    "@types/swiper": "^5.3.2",
    "axios": "^0.19.2",
    "gsap": "^3.6.0",
    "intersection-observer": "^0.7.0",
    "picturefill": "^3.0.3",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "7.9.0",
    "@deboxsoft/cpx": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.7",
    "chokidar-cli": "^2.1.0",
    "core-js": "^3.6.5",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-prettier": "^3.1.2",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.1.0",
    "lazysizes": "^5.2.0",
    "postcss": "^7.0.18",
    "postcss-calc": "^7.0.1",
    "postcss-cli": "^7.1.0",
    "postcss-color-function": "^4.1.0",
    "postcss-comment": "^2.0.0",
    "postcss-conditionals": "^2.1.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^9.0.2",
    "postcss-each": "^0.10.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.2",
    "postcss-nested": "^4.1.2",
    "postcss-simple-vars": "^5.0.2",
    "postcss-size": "^3.0.0",
    "prettier": "^2.0.2",
    "promise-polyfill": "^8.1.3",
    "pug-cli": "^1.0.0-alpha6",
    "stylelint": "^13.2.1",
    "stylelint-cli": "^1.3.0",
    "stylelint-config-standard": "^20.0.0",
    "swiper": "^5.3.7",
    "throttle-debounce": "^2.1.0",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "#"
  }
}
